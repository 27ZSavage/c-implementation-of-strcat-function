#define _CRT_SECURE_NO_WARNINGS 1
#include<stdio.h>
#include<string.h>
#include<assert.h>

char* my_strcat(char* arr1, const char* arr2)
{
	assert(arr1 && arr2);
	int* res = arr1;
	while (*arr1 != '\0')
	{
		arr1++;
		printf("%s\n", arr1);
	}
	while (*arr1++ = *arr2++)
	{
		;
	}
	return res;
}
int main()
{
	 char arr1[30] = "asdf";
	 char arr2[] = "kk";
	 my_strcat(arr1, arr2);
	 printf("%s\n", arr1);
	 return 0;
}
